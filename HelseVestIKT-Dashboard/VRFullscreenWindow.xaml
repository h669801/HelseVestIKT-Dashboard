<Window x:Class="HelseVestIKT_Dashboard.VRFullscreenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelseVestIKT_Dashboard"
		xmlns:wfi="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
        mc:Ignorable="d"
        Title="VRFullscreenWindow" Height="650" Width="1000"
		WindowStyle="SingleBorderWindow"
		Topmost="True">

	<Grid x:Name="FullscreenGrid" ClipToBounds="True">
		<wfi:WindowsFormsHost x:Name="VRHostFullscreen" Visibility="Visible"/>
	</Grid>
</Window>
