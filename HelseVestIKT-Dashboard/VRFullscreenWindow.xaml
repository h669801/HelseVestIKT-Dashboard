<Window x:Class="HelseVestIKT_Dashboard.VRFullscreenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelseVestIKT_Dashboard"
        mc:Ignorable="d"
        Title="VRFullscreenWindow" Height="450" Width="800"
		WindowStyle="None"
		Topmost="True">

	<Grid x:Name="FullscreenGrid" ClipToBounds="True">
		<TextBlock Text="LIVE VIEW" FontSize="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
	</Grid>
</Window>
